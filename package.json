{
  "name": "ice-koa-react-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "client": "ice dev --project-type=node",
    "server": "nodemon server/app.js",
    "start": "npm run client & npm run server",
    "build": "ice build --project-type=node",
    "lint": "eslint . --ext '.js,.jsx' --fix"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com",
    "access": "public"
  },
  "buildConfig": {
    "theme": "@icedesign/skin",
    "entry": "client/index.js",
    "localization": false
  },
  "proxyConfig": {
    "/api": {
      "enable": true,
      "target": "http://127.0.0.1:3000"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-respond": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "@icedesign/base": "^0.2.0",
    "@icedesign/container": "^0.1.4",
    "@icedesign/layout": "^0.1.2",
    "@icedesign/menu": "^0.1.1",
    "@icedesign/skin": "^0.1.0",
    "classnames": "^2.2.5",
    "foundation-symbol": "^0.1.3",
    "prop-types": "^15.5.8",
    "rc-animate": "^2.4.4",
    "rc-queue-anim": "^1.6.5",
    "rc-scroll-anim": "^2.5.5",
    "rc-texty": "^0.1.1",
    "rc-tween-one": "^2.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "koa-logger": "^3.2.0",
    "nodemon": "^1.18.4",
    "ice-scripts": "^1.6.0-next.3"
  },
  "title": "官网",
  "themeConfig": {
    "primaryColor": "#236cff"
  },
  "keywords": [
    "ice-scaffold"
  ],
  "templateType": "Koa"
}
